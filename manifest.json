{
  "name": "Address to Google Maps",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Create a Google Maps query from selection as address",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ]
}
